{
  "name": "demo-webdriverio-mocha",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "git@github.com:Nallikaea/demo-webdriverio-mocha.git"
  },
  "author": "Warley Gabriel da Paix√£o, Maryna Dolhalova",
  "scripts": {
    "build": "npx del ./dist && npx tsc",
    "watch": "npx tsc -w",
    "commit": "cz",
    "report:generate": "npx allure generate --clean ./test-report/allure-result/ -o ./test-report/allure-report",
    "report:open": "npx allure open test-report/allure-report",
    "run tests:": "npx del test-report && wdio ./dist/wdio.conf.js --spec ./src/test/suits/*.ts"
  },
  "dependencies": {
    "@reportportal/agent-js-webdriverio": "^5.0.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@wdio/allure-reporter": "^7.2.1",
    "@wdio/cli": "^7.2.3",
    "@wdio/local-runner": "^7.2.3",
    "@wdio/mocha-framework": "^7.2.1",
    "@wdio/spec-reporter": "^7.2.1",
    "@wdio/sync": "^7.2.3",
    "allure-commandline": "^2.13.8",
    "chromedriver": "^98.0.0",
    "cz": "^1.8.2",
    "del-cli": "^3.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "fibers": "^5.0.0",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "wdio-chromedriver-service": "^7.0.0",
    "wdio-image-comparison-service": "^2.3.0",
    "wdio-performancetotal-service": "^1.0.10",
    "wdio-reportportal-reporter": "^7.4.0",
    "wdio-reportportal-service": "^7.3.1",
    "wdio-slack-service": "^2.0.9",
    "wdio-testrail-reporter": "^1.1.5",
    "wdio-wdiov5testrail-reporter": "^1.0.30",
    "webdriverio": "^7.16.14"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all code:format build",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
