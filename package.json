{
  "name": "demo-webdriverio-mocha",
  "version": "0.0.1",
  "description": "Demo Integration Tests with WebdriverIO and Mocha",
  "repository": {
    "type": "git",
    "url": "git@github.com:WarleyGabriel/demo-webdriverio-mocha.git"
  },
  "license": "ISC",
  "author": "Warley Gabriel da Paix√£o",
  "main": "index.js",
  "scripts": {
    "check": "yarn run lint && yarn run prettier",
    "e2e:tests": "wdio wdio.conf.js",
    "format": "yarn run lint --fix --quiet && yarn run prettier --write",
    "lint": "eslint .",
    "prettier": "prettier --check \"**/*.js*\"",
    "report": "allure generate --clean ./test-report/allure-result/ -o ./test-report/allure-report",
    "visual:regression:tests": "wdio wdio.conf.js --spec test/visualRegression/*.spec.js"
  },
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.0",
    "@babel/register": "^7.4.4",
    "@types/node": "^12.0.12",
    "@wdio/allure-reporter": "^5.11.0",
    "@wdio/cli": "^5.11.0",
    "@wdio/dot-reporter": "^5.11.0",
    "@wdio/local-runner": "^5.11.0",
    "@wdio/mocha-framework": "^5.11.0",
    "@wdio/selenium-standalone-service": "^5.11.2",
    "@wdio/spec-reporter": "^5.11.0",
    "@wdio/sync": "^5.11.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "fibers": "^4.0.1",
    "husky": "^3.0.1",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
    "wdio-image-comparison-service": "^1.2.0",
    "wdio-timeline-reporter": "^5.0.10",
    "webdriverio": "^5.11.0"
  }
}
